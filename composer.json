{
    "name": "lloricode/laravel-paymaya-sdk",
    "description": "SDK for Paymaya using Laravel",
    "keywords": [
        "lloricode",
        "paymaya",
        "sdk",
        "laravel"
    ],
    "homepage": "https://github.com/lloricode/laravel-paymaya-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Lloric Mayuga Garcia",
            "email": "lloricode@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "lloricode/paymaya-sdk-php": "^0.2.1",
        "spatie/laravel-package-tools": "^1.1"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Lloricode\\LaravelPaymaya\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lloricode\\LaravelPaymaya\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lloricode\\LaravelPaymaya\\LaravelPaymayaServiceProvider"
            ],
            "aliases": {
                "PaymayaSDKClient": "Lloricode\\LaravelPaymaya\\Facades\\PaymayaClientFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
